public class TipoOperacionEnum{

	public enum Operaciones{
		CREARUSUARIOEXTERNO                     (1),
		ELIMINARUSUARIOEXTERNO                  (2);

		
		private final Integer operacion;

		private Operaciones(Integer operacion) {
			this.operacion = operacion;
		}
		public Integer getOperacion() {
			return operacion;
		}
	}
}
